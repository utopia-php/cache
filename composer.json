{
    "name": "utopia-php/cache",
    "description": "A simple cache library to manage application cache storing, loading and purging",
    "type": "library",
    "keywords": ["php","framework", "upf", "utopia", "cache"],
    "license": "MIT",
    "minimum-stability": "stable",
    "scripts": {
        "check": "./vendor/bin/phpstan analyse --level max src tests",
        "lint": "./vendor/bin/pint --test",
        "format": "./vendor/bin/pint",
        "test": "docker-compose exec -T php8 vendor/bin/psalm --show-info=true",
        "analyze": "docker-compose exec -T php8 vendor/bin/phpunit --configuration phpunit.xml"
    },
    "autoload": {
        "psr-4": {"Utopia\\Cache\\": "src/Cache"}
    },
    "autoload-dev": {
        "psr-4": {"Utopia\\Tests\\": "tests/Cache"}
    },
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-redis": "*",
        "ext-memcached": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "4.13.1",
        "phpstan/phpstan": "1.9.x-dev",
        "laravel/pint": "1.2.*"
    }
}
